<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c='urn:import:com.google.gwt.user.cellview.client'>
	<ui:style>
		.formPanel {
			border: 1px solid #889D9E;
		}
		
		.gridPanel {
			margin-top: 10px;
			border: 1px solid #889D9E;
		}
		
		.boxPanel {
			width: 400px;
			float: left;
			border: 1px solid #889D9E;
		}
		
		.leftPanel {
			width: 700px;
			float: right;
		}
	</ui:style>
	<g:HTMLPanel>

		<g:HTMLPanel styleName="{style.boxPanel}" ui:field="boxPanel" />
		<g:HTMLPanel styleName="{style.leftPanel}">
			<g:HTMLPanel styleName="{style.formPanel}" ui:field="formPanel" />
			<g:HTMLPanel styleName="{style.gridPanel}" ui:field="gridPanel" />
		</g:HTMLPanel>
	</g:HTMLPanel>

</ui:UiBinder>